{
  "name": "@bifravst/eslint-config-typescript",
  "version": "0.0.0-development",
  "description": "ESLint config for all Bifravst projects.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/eslint-config-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/eslint-config-typescript/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint-config-prettier": "^6.11.0",
    "snyk": "^1.370.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-angular": "9.1.2",
    "eslint": "7.8.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1"
  },
  "peerDependencies": {
    "eslint": "^7.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yml,md,js,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "test": "eslint --ext .js,.ts,.tsx *.js"
  },
  "snyk": true
}
